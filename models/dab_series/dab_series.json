{
  "type": "system",
  "name": "dab_series",
  "models": [
    {
      "name": "mv_dab",
      "type": "sd_dab",
      "sampling_frequency": 25e3
    },
    {
      "name": "mv_bus",
      "type": "bus_model",
      "sampling_frequency": 25e3
    },
    {
      "name": "lv_dab",
      "type": "sd_dab",
      "sampling_frequency": 25e3
    }
  ],
  "connections": [
    {
      "source": "mv_dab.v_out",
      "destination": "mv_bus.v_in"
    },
    {
      "source": "mv_bus.i_in",
      "destination": "mv_dab.i_out"
    },
    {
      "source": "mv_bus.v_out",
      "destination": "lv_dab.v_in"
    },
    {
      "source": "lv_dab.i_in",
      "destination": "mv_bus.i_out"
    }
  ],
  "inputs": [
    {
      "name": "mv_dab.v_in",
      "type": "constant",
      "value": 1000
    },
    {
      "name": "lv_dab.i_out",
      "type": "random",
      "distribution": {
        "name": "gaussian",
        "parameters": [150.0, 4]
      }
    }
  ],
  "outputs": [
    "mv_dab.i_out",
    "lv_dab.v_out"
  ]
}